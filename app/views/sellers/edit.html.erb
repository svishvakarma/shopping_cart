<h1>New Product</h1>

<%= form_with model: @product, url:seller_path(@product), method: :patch do |form| %>
<div>
  <%= form.label :title %><br>
  <%= form.text_field :title %>
</div>

<div>
  <%= form.label :discription %><br>
  <%= form.text_area :discription %>
</div>
<div>
  <%= form.label :price %><br>
  <%= form.text_field :price %>
</div>

<div>
  <%= form.submit %>
</div>
<% end %>
